{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/playstore",
        "destination": "https://play.google.com/store/apps/details?id=com.albumquestionbank"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
